//for each
let array = [1,2,3];
array.forEach(function (value) {
  console.log(value);
});
// 1
// 2
// 3
// for
let array = [1,2,3];
for (let i = 0; i < array.length; i++) {
  console.log(array[i]);
}


/// for in
var obj = {a:1,b:2};
for (let prop in obj) {
    console.log(prop);
}
// a
// b

let array = [10,20,30];
for (let index in array) {
  console.log(index);
});
// 0
